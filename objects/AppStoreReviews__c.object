    <webLinks>
        <fullName>Find_Intent</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Find Intent</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)} 

var resp = sforce.apex.execute( 
&quot;EinsteinServices&quot;, 
&quot;predictIntent&quot;, 
{ 
&quot;textToPredict&quot; : &quot;{!AppStoreReviews__c.Content__c}&quot; 
} 
); 

window[ resp[0].probabilities[0].label ] = resp[0].probabilities[0].probability; 
window[ resp[0].probabilities[1].label ] = resp[0].probabilities[1].probability; 
window[ resp[0].probabilities[2].label ] = resp[0].probabilities[2].probability; 

alert(resp[0].probabilities[0].label);</url>
    </webLinks>
    <webLinks>
        <fullName>Find_Sentiment</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Find Sentiment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)} 

var resp = sforce.apex.execute( 
    &quot;EinsteinServices&quot;, 
    &quot;findSentiment&quot;, 
    { 
        &quot;text&quot; : &quot;{!AppStoreReviews__c.Content__c}&quot; 
    } 
); 

window[ resp[0].probabilities[0].label ] = resp[0].probabilities[0].probability; 
window[ resp[0].probabilities[1].label ] = resp[0].probabilities[1].probability; 
window[ resp[0].probabilities[2].label ] = resp[0].probabilities[2].probability; 

var result = &quot;&quot;; 
if( negative &gt; positive &amp;&amp; negative &gt; neutral ) { 
    result = &quot;Negative&quot;; 
} 
else if( positive &gt; negative &amp;&amp; positive &gt; neutral ) { 
    result = &quot;Positive&quot;; 
} 
else { 
    result = &quot;Neutral&quot;; 
} 

alert( result );</url>
    </webLinks>
